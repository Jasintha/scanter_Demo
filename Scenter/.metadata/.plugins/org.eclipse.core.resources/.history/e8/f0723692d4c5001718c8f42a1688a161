    <property description="apiUrl" expression="json-eval($.sapiUrl)" name="apiUrl" scope="default" type="STRING"/>
    <property description="type" expression="json-eval($.stype)" name="type" scope="default" type="STRING"/>
    <property description="serverRelativeUrl" expression="json-eval($.sserverRelativeUrl)" name="serverRelativeUrl" scope="default" type="STRING"/>
    <property description="blocking" expression="json-eval($.sblocking)" name="blocking" scope="default" type="STRING"/>
